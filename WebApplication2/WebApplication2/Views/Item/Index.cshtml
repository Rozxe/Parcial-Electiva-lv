@{
    ViewData["Title"] = "Lista de productos";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    @foreach (var i in Model)
    {
        <h1>@ViewBag.ItemList</h1>
        <b>ID : </b> @i.ID <br />
        <b>Name : </b> @i.Name <br />
        <b>Category : </b> @i.Category <br />
        <b>Price : </b> @i.Price <br />
        <hr />
    }
</div>
